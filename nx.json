{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"affected": {
		"defaultBase": "master"
	},
	"generators": {
		"@nx/angular:application": {
			"e2eTestRunner": "cypress",
			"linter": "eslint",
			"style": "scss",
			"unitTestRunner": "jest"
		},
		"@nx/angular:component": {
			"style": "scss"
		},
		"@nx/angular:library": {
			"linter": "eslint",
			"unitTestRunner": "jest"
		}
	},
	"namedInputs": {
		"default": ["sharedGlobals", "{projectRoot}/**/*"],
		"production": [
			"!{projectRoot}/**/*.cy.[jt]s?(x)",
			"!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
			"!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
			"!{projectRoot}/.eslintrc.json",
			"!{projectRoot}/.storybook/**/*",
			"!{projectRoot}/.stylelintrc(.(json|yml|yaml|js))?",
			"!{projectRoot}/cypress.config.[jt]s",
			"!{projectRoot}/cypress/**/*",
			"!{projectRoot}/eslint.config.js",
			"!{projectRoot}/jest.config.[jt]s",
			"!{projectRoot}/src/test-setup.[jt]s",
			"!{projectRoot}/test-setup.[jt]s",
			"!{projectRoot}/tsconfig.spec.json",
			"!{projectRoot}/tsconfig.storybook.json",
			"default"
		],
		"sharedGlobals": [],
		"workspace": [
			"{workspaceRoot}/!(apps|libs|packages)/**/*",
			"{workspaceRoot}/*"
		]
	},
	"nxCloudAccessToken": "NDUxODJmOGItOTVhNi00OWEyLTkxZDEtZmI1ZTZjOTkxMDQzfHJlYWQ=",
	"parallel": 4,
	"targetDefaults": {
		"@nx/jest:jest": {
			"cache": true,
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			},
			"inputs": [
				"^production",
				"default",
				"{workspaceRoot}/jest.preset.js"
			],
			"options": {
				"passWithNoTests": false
			}
		},
		"build": {
			"cache": true,
			"dependsOn": ["^build"],
			"inputs": ["^production", "production"]
		},
		"compodoc": {
			"cache": true,
			"configurations": {
				"build": {
					"command": "{args.base}"
				},
				"coverage": {
					"command": "{args.base} --coverageTest 0 --silent"
				}
			},
			"inputs": ["^production", "default"]
		},
		"e2e": {
			"cache": true,
			"inputs": ["^production", "default"]
		},
		"lint": {
			"cache": true,
			"inputs": [
				"default",
				"{workspaceRoot}/.eslintignore",
				"{workspaceRoot}/.eslintrc.json",
				"{workspaceRoot}/eslint.config.js"
			],
			"options": {
				"maxWarnings": 15
			}
		},
		"storybook-build": {
			"cache": true,
			"inputs": [
				"^production",
				"default",
				"{projectRoot}/.storybook/**/*",
				"{projectRoot}/tsconfig.storybook.json"
			]
		},
		"stylelint": {
			"cache": true,
			"inputs": ["default", "{workspaceRoot}/.stylelintrc.json"],
			"options": {
				"maxWarnings": 15
			}
		},
		"typecheck": {
			"cache": true,
			"inputs": ["default"]
		}
	}
}
