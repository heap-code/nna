{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"name": "nest",
	"projectType": "library",
	"sourceRoot": "packages/nest/src",
	"tags": [],
	"targets": {
		"compodoc": {
			"executor": "nx:run-commands",
			"options": {
				"args": "--base='npx @compodoc/compodoc -p ./packages/nest/tsconfig.lib.json -d ./dist/compodoc/packages/nest --disablePrivate --disableInternal --disableLifeCycleHooks'",
				"command": "{args.base} -r 10001 -s"
			},
			"outputs": ["{workspaceRoot}/dist/compodoc/packages/nest"]
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		},
		"test": {
			"executor": "@nx/jest:jest",
			"options": {
				"jestConfig": "packages/nest/jest.config.ts"
			},
			"outputs": ["{workspaceRoot}/dist/coverage/{projectRoot}"]
		}
	}
}
