{
	"compilerOptions": {
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"module": "commonjs",
		"noFallthroughCasesInSwitch": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noPropertyAccessFromIndexSignature": true,
		"outDir": "../../dist/out-tsc",
		"sourceMap": false,
		"strict": true,
		"types": ["cypress", "node"]
	},
	"extends": "../../tsconfig.base.json",
	"include": [
		"**/*.cy.js",
		"**/*.cy.ts",
		"**/*.d.ts",
		"**/*.js",
		"**/*.ts",
		"cypress.config.ts"
	]
}
