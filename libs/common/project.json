{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"name": "common",
	"projectType": "library",
	"sourceRoot": "libs/common/src",
	"tags": [],
	"targets": {
		"compodoc": {
			"executor": "nx:run-commands",
			"options": {
				"args": "--base='npx @compodoc/compodoc -p ./libs/common/tsconfig.lib.json -d ./dist/compodoc/libs/common --disablePrivate --disableInternal --disableLifeCycleHooks'",
				"command": "{args.base} -r 10002 -s"
			},
			"outputs": ["{workspaceRoot}/dist/compodoc/libs/common"]
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		},
		"test": {
			"executor": "@nx/jest:jest",
			"options": {
				"jestConfig": "libs/common/jest.config.ts"
			},
			"outputs": ["{workspaceRoot}/dist/coverage/{projectRoot}"]
		},
		"typecheck": {
			"executor": "nx:run-commands",
			"options": {
				"command": "tsc --noEmit -p libs/common/tsconfig.spec.json"
			}
		}
	}
}
